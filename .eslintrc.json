{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
	    "quotes": "warn",
      "eol-last": "off",
      "no-unused-vars": "off",
      "require-jsdoc": "off",
      "no-trailing-spaces": "off",
      "padded-blocks": "off",
      "no-multiple-empty-lines": "off"
    }
}
